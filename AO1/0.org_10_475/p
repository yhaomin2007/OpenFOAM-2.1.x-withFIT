FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 10e+6;

boundaryField
{
    wall
    {
        type            zeroGradient;
    }
    inlet
    {
        //type            zeroGradient;
	type            fixedValue;
        value           uniform 10e+6;
    
	//type		totalPressure;
	//U		U;
	//phi		phi;
	//rho		rho;
	//gamma		1.3;
	//p0		10e+6;
}
    outlet
    {
        //type            fixedValue;
        //value           uniform 9.9e+6;
	type		uniformFixedValue;
	uniformValue	table
	(
(0 1e+07)
(1000 9.9e+06)
(5000 9.9e+06)
(6000 9.8e+06)
(10000 9.8e+06)
(11000 9.7e+06)
(15000 9.7e+06)
(16000 9.6e+06)
(20000 9.6e+06)
(21000 9.5e+06)
(25000 9.5e+06)
(26000 9.4e+06)
(30000 9.4e+06)
(31000 9.3e+06)
(35000 9.3e+06)
(36000 9.2e+06)
(40000 9.2e+06)
(41000 9.1e+06)
(45000 9.1e+06)
(46000 9e+06)
(50000 9e+06)
	);    

    }
    center
    {
	type		symmetryPlane;	
    }
    frontAndBack_pos
    {
        type            wedge;
    }
    frontAndBack_neg
    {
        type            wedge;
    }
}


